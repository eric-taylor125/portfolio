<h1>Listing tipo_de_usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_usuarios.each do |tipo_de_usuario| %>
      <tr>
        <td><%= tipo_de_usuario.nome %></td>
        <td><%= link_to 'Show', tipo_de_usuario %></td>
        <td><%= link_to 'Edit', edit_tipo_de_usuario_path(tipo_de_usuario) %></td>
        <td><%= link_to 'Destroy', tipo_de_usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

